{
  "name": "fullstack",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:dev": "env-cmd -f .local.env concurrently --kill-others \"yarn:db:up\" \"yarn:dev:api\" \"yarn:dev:web\" \"yarn:gen:gql\"",
    "dev:api": "yarn nx serve api",
    "dev:web": "yarn nx serve web",
    "DATABASE": "-----------------------------------------------------------------------------------------------",
    "db:up": "yarn db:docker && yarn db:dev-migrate && yarn db:create-client -- --watch",
    "db:docker": "docker-compose -f tools/docker-env/dev.docker-compose.yml up -d --no-recreate --remove-orphans",
    "db:create-client": "prisma generate",
    "db:dev-migrate": "env-cmd --no-override -f .local.env yarn prisma migrate dev",
    "db:studio": "env-cmd --no-override -f .local.env yarn prisma studio",
    "UTILS": "---------------------------------------------------------------------------------------------------",
    "gen:gql": "graphql-code-generator --config tools/gql-codegen/gql-codegen.yml --watch"
  },
  "private": true,
  "prisma": {
    "schema": "libs/api/data-access-db/src/lib/schema.prisma"
  },
  "dependencies": {
    "@fastify/cookie": "^8",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mercurius": "^10.1.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-fastify": "^9.0.11",
    "@prisma/client": "^4.3.1",
    "@urql/exchange-execute": "^2.0.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "core-js": "^3.6.5",
    "graphql": "^16.6.0",
    "helmet": "^6.0.0",
    "mercurius": "^11.0.0",
    "next": "12.3.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.12.1",
    "@graphql-codegen/near-operation-file-preset": "^2.4.1",
    "@graphql-codegen/typed-document-node": "^2.3.3",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-urql": "^3.7.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "14.7.5",
    "@nrwl/cypress": "14.7.11",
    "@nrwl/eslint-plugin-nx": "14.7.5",
    "@nrwl/jest": "14.7.5",
    "@nrwl/linter": "14.7.5",
    "@nrwl/nest": "^14.7.5",
    "@nrwl/next": "^14.7.11",
    "@nrwl/node": "14.7.5",
    "@nrwl/react": "14.7.11",
    "@nrwl/web": "14.7.11",
    "@nrwl/workspace": "14.7.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "28.1.1",
